{
	"name": "allergy-nav",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": "20.12.0",
		"npm": "10.5.0"
	},
	"scripts": {
		"dev": "WATCHPACK_POLLING=true next dev -p 10111",
		"build": "next build",
		"start": "next start -p 10111",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
		"format": "prettier --write './**/*' && prisma format",
		"lint:fix": "npm run lint -- --fix && npm run format",
		"tsnode": "ts-node -r tsconfig-paths/register --project ./tsconfig.node.json",
		"analyze": "ANALYZE=true npm run build",
		"db:deploy": "dotenv -e .env.development -- prisma migrate deploy",
		"db:generate": "dotenv -e .env.development -- prisma generate",
		"db:deploy:prod": "dotenv -e .env.production.local -- prisma migrate deploy",
		"db:migrate": "dotenv -e .env.development -- prisma migrate dev",
		"db:delete": "dotenv -e .env.development -- prisma migrate reset --skip-seed",
		"db:seed": "dotenv -e .env.development -- npm run tsnode -- prisma/seed.ts",
		"db:init": "npm run db:deploy && npm run db:seed",
		"batch:delete-noverified-account": "dotenv -e .env.development -- npm run tsnode -- batch/delete-noverified-account/batch.ts",
		"batch:delete-recovery-code": "dotenv -e .env.development -- npm run tsnode -- batch/delete-recovery-code/batch.ts",
		"batch:delete-verify-code": "dotenv -e .env.development -- npm run tsnode -- batch/delete-verify-code/batch.ts"
	},
	"dependencies": {
		"@akimeaki/check-browser": "0.1.0",
		"@akimeaki/reset-css": "0.3.0",
		"@aws-sdk/client-s3": "3.693.0",
		"@geolonia/normalize-japanese-addresses": "3.1.1",
		"@kuma-ui/core": "1.5.8",
		"@kuma-ui/next-plugin": "1.3.2",
		"@next-auth/prisma-adapter": "1.0.7",
		"@next/bundle-analyzer": "15.0.3",
		"@paralleldrive/cuid2": "2.2.2",
		"@prisma/client": "5.22.0",
		"@types/bcrypt": "5.0.2",
		"@types/node": "22.9.0",
		"@types/react": "18.2.15",
		"@types/react-dom": "18.2.7",
		"@types/sanitize-html": "2.13.0",
		"bcrypt": "5.1.1",
		"dotenv-cli": "7.4.2",
		"next": "14.2.1",
		"next-auth": "4.24.7",
		"nextjs-toploader": "3.7.15",
		"prisma": "5.22.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"resend": "3.2.0",
		"sanitize-html": "2.13.1",
		"sass": "1.81.0",
		"sharp": "0.33.5",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.6.3"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "15.0.3",
		"@typescript-eslint/parser": "8.15.0",
		"eslint": "9.15.0",
		"eslint-config-next": "15.0.3",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-react": "7.37.2",
		"eslint-plugin-react-hooks": "5.0.0",
		"globals": "15.12.0",
		"prettier": "3.3.3",
		"prettier-plugin-sh": "0.14.0",
		"typescript-eslint": "8.15.0"
	}
}
