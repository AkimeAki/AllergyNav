{
	"name": "allergy-nav",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": "20.12.0",
		"npm": "10.5.0"
	},
	"scripts": {
		"dev": "WATCHPACK_POLLING=true next dev -p 10111",
		"build": "next build",
		"start": "next start -p 10111",
		"lint": "ESLINT_USE_FLAT_CONFIG=true eslint .",
		"format": "prettier --write './**/*' && prisma format",
		"lint:fix": "npm run lint -- --fix && npm run format",
		"tsnode": "ts-node -r tsconfig-paths/register --project ./tsconfig.node.json",
		"db:deploy": "dotenv -e .env.development -- prisma migrate deploy",
		"db:generate": "dotenv -e .env.development -- prisma generate",
		"db:deploy:prod": "dotenv -e .env.production.local -- prisma migrate deploy",
		"db:migrate": "dotenv -e .env.development -- prisma migrate dev",
		"db:delete": "dotenv -e .env.development -- prisma migrate reset --skip-seed",
		"db:seed": "dotenv -e .env.development -- npm run tsnode -- prisma/seed.ts",
		"db:init": "npm run db:deploy && npm run db:seed"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "3.554.0",
		"@geolonia/normalize-japanese-addresses": "2.10.1",
		"@kuma-ui/core": "1.5.4",
		"@kuma-ui/next-plugin": "1.1.14",
		"@next-auth/prisma-adapter": "1.0.7",
		"@paralleldrive/cuid2": "2.2.2",
		"@prisma/client": "5.11.0",
		"@types/bcrypt": "5.0.1",
		"@types/node": "20.7.1",
		"@types/react": "18.2.15",
		"@types/react-dom": "18.2.7",
		"@types/sanitize-html": "2.11.0",
		"bcrypt": "5.1.1",
		"dotenv-cli": "7.4.2",
		"next": "14.2.1",
		"next-auth": "4.24.7",
		"nextjs-toploader": "1.6.12",
		"prisma": "5.11.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"resend": "3.2.0",
		"sanitize-html": "2.11.0",
		"sass": "1.77.2",
		"sharp": "0.33.3",
		"ts-node": "10.9.2",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.0.2"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "15.0.3",
		"@typescript-eslint/parser": "8.13.0",
		"encoding": "0.1.13",
		"eslint": "8.57.0",
		"eslint-config-next": "14.2.1",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-react": "7.37.2",
		"eslint-plugin-react-hooks": "5.0.0",
		"globals": "15.12.0",
		"prettier": "3.0.3",
		"prettier-plugin-sh": "0.14.0",
		"typescript-eslint": "8.13.0"
	}
}
