{
	"name": "allergy-nav",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": "20.12.0",
		"npm": "10.5.0"
	},
	"scripts": {
		"dev": "WATCHPACK_POLLING=true next dev -p 10111",
		"build": "next build",
		"start": "next start -p 10111",
		"lint": "eslint \"./**/*.{js,cjs,mjs,ts,tsx}\"",
		"format": "prettier --write './**/*.{json,js,cjs,mjs,ts,tsx,yaml,yml,html}' && prisma format",
		"lint:fix": "npm run lint -- --fix && npm run format",
		"tsnode": "ts-node -r tsconfig-paths/register --project ./tsconfig.node.json",
		"prisma:deploy:dev": "dotenv -e .env.development -- prisma migrate deploy",
		"prisma:deploy:prod": "dotenv -e .env.production.local -- prisma migrate deploy",
		"prisma:migrate": "dotenv -e .env.development -- prisma migrate dev",
		"prisma:delete": "dotenv -e .env.development -- prisma migrate reset --skip-seed",
		"prisma:seed": "dotenv -e .env.development -- npm run tsnode -- prisma/seed.ts",
		"prisma:studio": "prisma studio",
		"init": "npm run prisma:deploy:dev && npm run prisma:seed"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.554.0",
		"@geolonia/normalize-japanese-addresses": "^2.10.1",
		"@kuma-ui/core": "^1.5.4",
		"@kuma-ui/next-plugin": "^1.1.14",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@paralleldrive/cuid2": "^2.2.2",
		"@prisma/client": "^5.11.0",
		"@types/bcrypt": "^5.0.1",
		"@types/node": "^20.7.1",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/sanitize-html": "^2.11.0",
		"bcrypt": "^5.1.1",
		"dotenv-cli": "^7.4.2",
		"next": "^14.2.1",
		"next-auth": "^4.24.7",
		"nextjs-toploader": "^1.6.12",
		"prisma": "^5.11.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"resend": "^3.2.0",
		"sanitize-html": "^2.11.0",
		"sass": "^1.77.2",
		"sharp": "^0.33.3",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.2"
	},
	"devDependencies": {
		"encoding": "^0.1.13",
		"eslint": "^8.45.0",
		"eslint-config-next": "^14.2.1",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.0.3"
	}
}
